@page
@model RecipeApplication.Pages.UsersListModel
@{
}

@foreach (var item in Model.model)
{
    <div>User: @item.UserName</div>
    <div>Email: @item.Email</div>
    <form asp-page-handler="DeleteUser" method="post" asp-route-stud_id="@item.UserName" onsubmit="return confirm('Delete?');">
        <button type="submit" class="btn btn-primary">Delete</button>
    </form>
    <br />
}
